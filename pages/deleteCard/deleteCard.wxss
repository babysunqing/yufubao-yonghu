/* pages/deleteCard/deleteCard.wxss */
page{
  background-color: #f0f0f0;
}
.container{
  padding: 32rpx 64rpx 0 64rpx
}
.content{
  width: 100%;
  height: 780rpx;
  background-color: #fff;
  border-radius: 10px;
  box-shadow: 0 1px 4px 0 rgba(88,88,88,0.10);
  padding: 34rpx 32rpx 0 32rpx;
}
.InputItem{
  width: 100%;
  height: 104rpx;
  border-bottom: 1px #f0f0f0 solid;
}
.InputItem .title{
  font-size: 28rpx;
  color: #333;
  margin-bottom: 6rpx;
}
.placeholder{
  font-size: 32rpx;
  color: #ccc;
}
.banlence{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx
}
.left{
  font-size: 30rpx;
  color: #333;
  float: left
}
.right{
  font-size: 30rpx;
  color: #333;
  float: right
}
.firstBanlence{
  margin-top:48rpx;
}
.freeBanlence{
  padding-bottom: 16rpx;
  border-bottom: 1px #f0f0f0 solid
}
.done{
  width: 100%;
  height: 88rpx;
  margin-top: 80rpx;
  background-color: #BF793A;
  color: #fff;
  font-size: 34rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 6px;
}